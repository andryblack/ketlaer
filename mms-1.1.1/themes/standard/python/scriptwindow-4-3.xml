<window>
   <id>100</id>
   <defaultcontrol>10</defaultcontrol>
   <width>800</width>
   <height>600</height>
   <controls>
      <control>
         <description>Background image</description>
         <type>image</type>
         <id>1</id>
         <posx>0</posx>
         <posy>0</posy>
         <width>800</width>
         <height>600</height>
         <layer>0</layer>
	 <routecontrol>10</routecontrol>
	 <texture>python/bg-4-3.png</texture>
      </control>

      <control>
         <description>Plugin image</description>
         <type>image</type>
         <id>2</id>
         <posx>25</posx>
         <posy>10</posy>
         <width>50</width>
         <height>50</height>
         <layer>3</layer>
         <texture>python/script.png</texture>
      </control>

      <control>
         <description>Plugin label</description>
         <type>label</type>
         <id>3</id>
         <posx>90</posx>
         <posy>10</posy>
         <width>200</width>
         <height>50</height>
         <layer>4</layer>
	 <label>Scripts</label>
         <textcolor>0xffffff</textcolor>
      </control>

      <control>
         <description>Center rectangle</description>
         <type>rectangle</type>
         <id>4</id>
         <posx>50</posx>
         <posy>90</posy>
         <width>700</width>
         <height>430</height>
         <layer>3</layer>
	 <alpha>15</alpha>
         <rectanglecolor>0xdedede</rectanglecolor>
      </control>

      <control>
         <description>Script list</description>
         <type>list</type>
         <id>10</id>
         <posx>70</posx>
         <posy>105</posy>
         <width>660</width>
         <height>420</height>
         <layer>4</layer>
	 <texturefocus>movie/marked.png</texturefocus>
         <textoffset>8</textoffset>
         <itemheight>30</itemheight>
      </control>
   </controls>
</window>
