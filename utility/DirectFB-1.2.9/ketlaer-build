export PKG_CONFIG_PATH="$KETLAER/lib/pkgconfig"
export PATH="$KETLAER/bin:$PATH"
export CFLAGS="-mips32 -O3 -I$KETLAER/include -I$KETLAER_DIR/libketlaer"
export CPPFLAGS="$CFLAGS"
export LDFLAGS="-L$KETLAER/lib"
export LIBS=""
./configure --prefix=$KETLAER --host=mipsel-linux --disable-devmem --enable-rtd --disable-fbdev --with-gfxdrivers=none --with-inputdrivers=linuxinput --disable-video4linux
make -i install
